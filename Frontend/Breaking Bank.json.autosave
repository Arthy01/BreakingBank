sole.log(jwt);","    // Die Verbindung wird nun im globalen window-Objekt gespeichert","    window.connection = new signalR.HubConnectionBuilder()","        .withUrl(\"https://breakingbank.de:5001/gameHub\", {","            transport: signalR.HttpTransportType.WebSockets,","            accessTokenFactory: () => jwt","        })","        .withAutomaticReconnect()","        .build();","    ","    window.connection.on(\"ReceiveMessage\", (message) => {","        console.log(\"[RECEIVE MESSAGE] \" + message);","    });","","    window.connection.on(\"ForceDisconnect\", () => ForceDisconnect(window.connection));","","    window.connection.on(\"ReceiveSaveGame\", (data) => {","        runtimeScene.getGame().getVariables().get(\"jsonsavegame\").setString(JSON.stringify(data, null, 2));","        runtimeScene.getGame().getVariables().get(\"receivedjsonsavegame\").setNumber(1); ","    });","","    window.connection.on(\"ReceiveTick\", (dataTick) => {","        runtimeScene.getGame().getVariables().get(\"receivedTicks\").setString(JSON.stringify(dataTick, null, 2));","        runtimeScene.getGame().getVariables().get(\"tickReceived\").setNumber(1); ","    });","","    window.connection.start()","        .then(() => {","            console.log(\"Verbindung erfolgreich hergestellt!\");","            runtimeScene.getVariables().get(\"loadedSavegame\").setNumber(1);","        })","        .catch(err => {","            console.error(\"Fehler beim Verbinden:\", err);","        });","}","","async function createSaveGame() {","    const requestOptions = {","        method: \"POST\",","        headers: {","            \"Authorization\": `Bearer ${jwt}`,  // JWT im Authorization-Header senden","        },","        redirect: \"follow\"","    };","","    try {","        const response = await fetch(`https://breakingbank.de:5001/api/SaveGame/create?saveGameName=${saveGameName}`, requestOptions);","        const result = await response.text();","        console.log(result);  // Gesamtantwort ausgeben","","        // Die Antwort an \"id \" splitten","        const parts = result.split(\"id \");","","        if (parts.length > 1) {","            saveGameID = parts[1].trim();  // ID extrahieren und speichern","            console.log(saveGameID);","            runtimeScene.getGame().getVariables().get(\"savegameID\").getChild(0).setString(saveGameID);","        } else {","            console.error(\"ID nicht gefunden!\");","        }","    } catch (error) {","        console.error(\"Fehler beim Erstellen des Spielstandes:\", error);","    }","}","","async function createSession() {","    if (!saveGameID) {","        console.error(\"Speicher-ID ist leer, daher kann keine Sitzung erstellt werden.\");","        return;  // Beende die Funktion, wenn keine ID verfgbar ist","    }","","    const requestOptions = {","        method: \"POST\",","        headers: {","            \"Authorization\": `Bearer ${jwt}`,  // JWT im Authorization-Header senden","        },","        redirect: \"follow\"","    };","","    try {","        const response = await fetch(`https://breakingbank.de:5001/api/Session/create?saveGameID=${saveGameID}`, requestOptions);","        const result = await response.text();","        console.log(result);","    } catch (error) {","        console.error(\"Fehler beim Erstellen der Sitzung:\", error);","    }","}","","async function start() {","    await createSaveGame();  // Warten auf die Erstellung des SaveGames","    await createSession();    // Erstelle die Sitzung mit der erhaltenen ID","    await initializeSignalR(); // Initialisiere SignalR","}","","function ForceDisconnect(connection) {","    if (connection && connection.state === signalR.HubConnectionState.Connected) {","        console.warn(\"Verbindung wird aufgrund von 'ForceDisconnect' getrennt...\");","        runtimeScene.getGame().getVariables().get(\"Disconnect\").setNumber(1);","        connection.stop()","            .then(() => console.log(\"Verbindung erfolgreich getren